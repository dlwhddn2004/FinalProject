    <!-- Argon Scripts -->
	<!-- Core -->
	<script src="${pageContext.request.contextPath }/assets/vendor/jquery/dist/jquery.min.js"></script>
	
	<!-- My JavaScript -->
	<script type="text/javascript">
		if (${!empty param.message}) {
			Swal.fire(
			  '${param.taskResult}',
			  '${param.message}',
			  '${param.taskResult}'
			)
		}
	</script>
